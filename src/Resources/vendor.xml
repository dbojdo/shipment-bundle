<?xml version="1.0" encoding="UTF-8" ?>
<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services
        http://symfony.com/schema/dic/services/services-1.0.xsd"
        >
    <parameters>
        <parameter key="webit_shipment.repository.vendor.in_memory.class">Webit\Shipment\Vendor\VendorRepositoryInMemory</parameter>
        <parameter key="webit_shipment.repository.vendor.in_memory.factory.class">Webit\Shipment\Vendor\VendorRepositoryInMemoryFactory</parameter>
    </parameters>
    <services>
        <service
                id="webit_shipment.repository.vendor.in_memory_factory"
                class="%webit_shipment.repository.vendor.in_memory_factory.class%"
                public="false"
        />
        <service
                id="webit_shipment.repository.vendor.in_memory"
                class="%webit_shipment.repository.vendor.in_memory.class%"
                factory-service="webit_shipment.repository.vendor.in_memory_factory"
                factory-method="createVendorRepository"
        />
        <service id="webit_shipment.repository.vendor" alias="webit_shipment.repository.vendor.in_memory" />
    </services>
</container>
