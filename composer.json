{
    "name" : "webit/shipment-bundle",
    "type": "symfony-bundle",
    "description": "Web-IT Shipment Library Symfony 2 integration",
    "keywords": ["web-it","shipment", "bundle", "delivery", "parcel", "dispatch"],
    "homepage": "http://www.web-it.eu",
    "license": "MIT",
    "authors": [
        {
            "name": "Daniel Bojdo",
            "email": "daniel.bojdo@web-it.eu",
            "homepage": "http://www.web-it.eu"
        }
    ],
    "require": {
        "php": ">=5.3.2",
        "symfony/symfony": "~2.3",
        "doctrine/doctrine-bundle": "~1.2",
        "doctrine/orm": ">=2.2,<2.5",
        "webit/shipment": "dev-master"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.2",
        "behat/behat": "~3.0",
        "behat/symfony2-extension": "~2.0",
        "jms/serializer-bundle": "~0.12"
    },
    "autoload": {
        "psr-4": {
            "Webit\\Bundle\\ShipmentBundle\\" : "src"
        }
    },
    "minimum-stability": "dev",
    "suggest": {
        "jms/serializer-bundle": "JMS Serializer support"
    }
}
