{
    "name" : "webit/shipment-bundle",
    "type": "symfony-bundle",
    "description": "Web-IT Shipment Library Symfony 2 integration",
    "keywords": ["web-it","shipment", "bundle", "delivery", "parcel", "dispatch"],
    "homepage": "http://www.web-it.eu",
    "license": "MIT",
    "authors": [
        {
            "name": "Daniel Bojdo",
            "email": "daniel.bojdo@web-it.eu",
            "homepage": "http://www.web-it.eu"
        }
    ],
    "require": {
        "php": ">=5.4.16",
        "symfony/symfony": "^2.5|^3.0",
        "doctrine/doctrine-bundle": "^1.4",
        "doctrine/orm": "^2.4",
        "webit/shipment": "^3.0.0"
    },
    "require-dev": {
        "webit/test-tools": "*",
        "phpunit/phpunit": "~4.2",
        "behat/behat": "~3.0",
        "jms/serializer-bundle": "~1.0"
    },
    "autoload": {
        "psr-4": {
            "Webit\\Bundle\\ShipmentBundle\\" : "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Webit\\Bundle\\ShipmentBundle\\Features\\" : "features"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "suggest": {
        "jms/serializer-bundle": "JMS Serializer support"
    },
    "extra": {
        "branch-alias": {
            "dev-1.x": "1.x-dev",
            "dev-2.x": "2.x-dev",
            "dev-master": "3.x-dev"
        }
    }
}
